  "devDependencies": {
    "@warren-bank/translate-android-strings": "^1.9.2"
  },
  "scripts": {
    "translate":       "translate-android-strings -i \"en\" -f \"android-studio-project/TV-Tracker/src/main/res/values/strings.xml\" -d \"android-studio-project/TV-Tracker/src/withAllLanguageTranslations/res\" -m --na --nc --nw -b \"(?:TV|IMDb|OK|TMDb|TMDB|TVDB|API|ID|DB)\"",
